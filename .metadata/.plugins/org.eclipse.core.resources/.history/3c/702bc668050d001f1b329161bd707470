package com.kosta.sample.quiz1;

public class Sukang extends Score {

	private int[] scores = new int[3];
	// int[] scores = {0,0,0};

	public Sukang() {
		//super();
	}
	
	
	public Sukang(int kor, int eng, int math) {
		super(kor, eng, math); //파라미터를 여기로넘기기
	}

	
	public Sukang(int[] scores) {
        super(scores);
    }
	
	
	
	public static void main(String[] a) {
		//Sukang s = new Sukang(100,90,80); //과목 점수만 출력됨
		new Score(100, 90, 80); //Score는 기본생성자X 부모생성자O,
		new Score(88, 77, 66); //Score말고 SuKang을 적어도 가능하게
		
		
		new Sukang (new int[]{50, 50, 50});
		
	}

}
