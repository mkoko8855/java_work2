package com.kosta.sample.pkg;

//패키지 = 폴더
//유사한 기능을 하는 클래스의 묶음
import com.kosta.sample.pkg.DelDel;

class Parent() {
	public Parent(){
		System.out.println("Parent() 생성자 콜");
	}
}


public class DelCall {

	public DelCall() {
		System.out.println("DelCall() 생성자 콜");
	}
	
	
	
	public static void main(String[] args) {

		DelDel d = new DelDel();
		d.myprint();
	
	}

}
