package com.kosta.sample.pkg;

//패키지 = 폴더
//유사한 기능을 하는 클래스의 묶음
import com.kosta.sample.pkg.DelDel;

class Parent {
    public Parent() {
        System.out.println("Parent() 생성자 콜");
    }
}


public class DelCall extends Parent {

	public DelCall() {
		super(); //자식의 생성자를 호출할 때 사용한다.
		System.out.println("DelCall() 생성자 콜");
	}
	
	
	
	public static void main(String[] args) {
		DelCall cd = new DelCall(); //출력결과는 Parent()와 DelCall() 같이 찍힌다.
		
		
		DelDel d = new DelDel();
		d.myprint();//출력결과는 ddd만 찍힌다.
		
	}

}
