      //list.add(emp);
	  //위 코드도 가능.
      
      
		
	  //또는
	  /*
	    ArrayList<HashMap<String, String>> list = new ArrayList<HashMap<String, String>>();
	    String[] arr = {"smith", "7733", "600"};
	    HashpMap getMap = list.get(0); 이러면 list에 hasp map을 담았기에 잘못됐다.
	    getMap.put("empno", arr[0]);
