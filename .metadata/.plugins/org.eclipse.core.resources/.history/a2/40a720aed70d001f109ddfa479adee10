package com.kosta.shop;

class User {
	
	private final int ORDER_POINT = 100;
	
	public int getOrderPoint() {
		return this.ORDER_POINT;
	}


	public String getUserName(String uid) {
		//DB조회 : SELECT ename FROM emp WHERE userid = 'uid';
		return "홍길동";
	}
	
	
	
	public int order(int price) { //거의빈껍데기에가까움
		int res = 0;
		return res;
	}
	
	
	public String getCompName() {
		return "KOSTA";
	}

}


class Member extends User {
	
	@Override
	public int order(int price) {
		int res = price + getOrderPoint() + (int)(price*0.1);
		return res;
	}
}



